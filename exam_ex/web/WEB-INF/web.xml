<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>hide</servlet-name>
        <servlet-class>hide</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>hide2</servlet-name>
        <servlet-class>hide2</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>life_cycle</servlet-name>
        <servlet-class>life_cycle</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>url_re</servlet-name>
        <servlet-class>url_re</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>url_re2</servlet-name>
        <servlet-class>url_re2</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>hide</servlet-name>
        <url-pattern>/hide</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>hide2</servlet-name>
        <url-pattern>/hide2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>life_cycle</servlet-name>
        <url-pattern>/life_cycle</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>url_re</servlet-name>
        <url-pattern>/url_re</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>url_re2</servlet-name>
        <url-pattern>/url_re2</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
